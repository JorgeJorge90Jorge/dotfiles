[user]
  email = patrick@soundcloud.com
  name = Patrick Ellis
[core]
  excludesfile = ~/.gitignore
  whitespace=fix,indent-with-non-tab,trailing-space,cr-at-eol
[push]
  default = current
[branch]
  autosetuprebase = always
[branch "master"]
  remote = origin
  merge = refs/heads/master
  rebase = true
[rerere]
  enabled = true
[alias]
  st = status
  co = checkout
  fx = !sh -c 'git commit --fixup=$1' -
[color]
  ui = true
[github]
  user = pje
[credential]
	helper = osxkeychain
[gist]
  private = yes
